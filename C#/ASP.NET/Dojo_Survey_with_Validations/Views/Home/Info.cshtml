@model Dojo_Survey_with_Validations.Models.Survey

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
</head>
<body>
    <div class="container">
        <div class="row border mt-5 p-3">
            <h1>Info:</h1>
            <div class="row">
                <div class="col-sm-12">
                    <p>Name:</p>
                </div>
                <br>
                <div class="col-sm-12">
                    <p>@Model.Name</p>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <p>Dojo Location:</p>
                </div>
                <br>
                <div class="col-sm-12">
                    <p>@Model.Location</p>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <p>Favorite Language:</p>
                </div>
                <br>
                <div class="col-sm-12">
                    <p>@Model.Language</p>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <p>Comment:</p>
                </div>
                <br>
                <div class="col-sm-12">
                    <p>@Model.Message</p>
                </div>
            </div>
        </div>
    </div>
</body>
</html>